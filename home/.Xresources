! Iosevka font face & size
! fc-list :scalable=true:spacing=mono: family | grep Iosevka
xterm*faceName: Iosevka Custom
xterm*faceSize: 12
xterm*renderFont: true

! RHS scrollbar
XTerm*ScrollBar: true
XTerm*rightScrollBar: true


! Colour scheme: overgrowth (dark variant)
